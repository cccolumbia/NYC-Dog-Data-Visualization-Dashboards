dog.bites.age$Age[dog.bites.age$Age=="2-3M"] <- "2.5M"
dog.bites.age$Age.new <- NA
dog.bites.age$Age.new <- ifelse(grepl("W",dog.bites.age$Age),0,NA)
dog.bites.age$Age.new <- ifelse(grepl("Y",dog.bites.age$Age),regmatches(dog.bites.age$Age,gregexpr('[0-9]+',dog.bites.age$Age)),NA)
View(dog.bites.age)
unique(dog.bites.age$Age.new)
