dog.bites.age$Age <- ifelse(grepl("Y",dog.bites.age$Age),regmatches(dog.bites.age$Age,gregexpr('[0-9]+',dog.bites.age$Age)),dog.bites.age$Age)
unique(dog.bites.age$Age)
dog.bites.age$Age <- as.numeric(dog.bites.age$Age)
